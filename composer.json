{
    "name": "webfactorybulgaria/base",
    "description": "Multilingual CMS built with Laravel 5",
    "keywords": ["cms", "multilingual", "laravel", "typi", "admintool4", "webfactorybulgaria"],
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel De Backer",
            "email": "samuel@typidesign.be",
            "role": "Developer"
        }
    ],
    "type": "project",
    "require": {
        "laravel/framework": "~5.1.0",
        "barryvdh/laravel-debugbar": "~2.0",
        "cviebrock/image-validator": "~2.0.0",
        "edvinaskrucas/notification": "~5.1.0",
        "guzzlehttp/guzzle": "~6.0",
        "laracasts/presenter": "~0.2.1",
        "laracasts/utilities": "~2.1.0",
        "bkwld/croppa": "~4.2.1",
        "maatwebsite/laravel-sidebar": "~1.1.1",
        "webfactorybulgaria/nestablecollection": "~1.1.1",
        "webfactorybulgaria/translatablebootforms": "~2.2.0",
        "webfactorybulgaria/core": "~4.0.2",
        "webfactorybulgaria/blocks": "~4.0.1",
        "webfactorybulgaria/dashboard": "~4.0.1",
        "webfactorybulgaria/files": "~4.0.1",
        "webfactorybulgaria/galleries": "~4.0.1",
        "webfactorybulgaria/groups": "~4.0.1",
        "webfactorybulgaria/history": "~4.0.1",
        "webfactorybulgaria/menus": "~4.0.1",
        "webfactorybulgaria/pages": "~4.0.2",
        "webfactorybulgaria/settings": "~4.0.1",
        "webfactorybulgaria/sitemap": "~4.0.1",
        "webfactorybulgaria/translations": "~4.0.1",
        "webfactorybulgaria/users": "~4.0.1"
    },
    "require-dev": {
        "laracasts/generators": "~1.1",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "TypiCMS\\Modules\\": "Modules/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php clear-compiled",
            "php artisan clear-html",
            "php artisan optimize"
        ],
        "pre-update-cmd": [
            "php clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan clear-html",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable"
}
